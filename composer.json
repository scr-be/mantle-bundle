{
    "name"        : "scribe/mantle-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/mantle-bundle",
    "description" : "An MIT licensed bundle used primarily by Scribe Inc for their own website and client sites. It includes the collection of packages required for our development needs.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://symfony-mantle-bundle.docs.scribe.tools/issues",
        "source" : "https://symfony-mantle-bundle.docs.scribe.tools/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "scribe-mantle-bundle", "mantle-bundle", "mantle", "foundation", "bundle", "symfony", "symfony-framework", "scribe", "scr.be", "scribenet", "scribe-software", "scribe-services"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        },
        {
            "name"    : "Dan Corrigan",
            "email"   : "dfc@scr.be",
            "homepage": "https://scr.be/dfc",
            "role"    : "Developer (Rogue Rubyist)"
        },
        {
            "name"    : "Andrew Thomas",
            "email"   : "aht@scr.be",
            "homepage": "https://scr.be/aht",
            "role"    : "Developer (UX)"
        }
    ],
    "require" : {
        "php"                                  : ">=5.6.4,~5.6",
        "scribe/wonka-library"                 : "~0.1@dev",
        "scribe/wonka-bundle"                  : "~0.1@dev",
        "symfony/symfony"                      : "~2.7|~2.8@dev",
        "symfony/framework-bundle"             : "~2.7|~2.8@dev",
        "symfony/yaml"                         : "~2.7|~2.8@dev",
        "symfony/filesystem"                   : "~2.7|~2.8@dev",
        "symfony/finder"                       : "~2.7|~2.8@dev",
        "symfony/dependency-injection"         : "~2.7|~2.8@dev",
        "symfony/monolog-bundle"               : "~2.7|~2.8@dev",
        "symfony/assetic-bundle"               : "~2.7|~2.8@dev",
        "kriswallsmith/assetic"                : "~1.3",
        "symfony/swiftmailer-bundle"           : "~2.3",
        "swiftmailer/swiftmailer"              : "~5.4",
        "doctrine/orm"                         : "~2.5",
        "doctrine/common"                      : ">=2.5.1,~2.5",
        "doctrine/dbal"                        : ">=2.5.1,~2.5",
        "doctrine/doctrine-bundle"             : ">=1.5.1,~1.5",
        "twig/twig"                            : ">=1.22,~1.22",
        "twig/extensions"                      : "~1.3",
        "sensio/distribution-bundle"           : "~4.0",
        "sensio/framework-extra-bundle"        : "~3.0",
        "incenteev/composer-parameter-handler" : "~2.1",
        "zendframework/zend-json"              : "~2.3"
    },
    "require-dev" : {
        "ext-memcached"                        : "~2.2",
        "ext-igbinary"                         : "~1.2",
        "ext-tidy"                             : "~2.0",
        "ext-curl"                             : "*",
        "ext-json"                             : "*",
        "ext-mysqli"                           : "*",
        "ext-pdo"                              : "*",
        "scribe/swim-bundle"                   : "~2.0@dev",
        "scribe/cache-bundle"                  : "~2.0@dev",
        "symfony/stopwatch"                    : "~2.7|~2.8@dev",
        "symfony/var-dumper"                   : "~2.7|~2.8@dev",
        "symfony/debug-bundle"                 : "~2.7|~2.8@dev",
        "symfony/phpunit-bridge"               : "~2.7|~2.8@dev",
        "phpunit/phpunit"                      : "~4.8",
        "sensio/generator-bundle"              : "~2.5",
        "doctrine/doctrine-fixtures-bundle"    : "~2.0@dev",
        "doctrine/data-fixtures"               : "~1.1",
        "doctrine/migrations"                  : "~1.0@dev",
        "doctrine/doctrine-migrations-bundle"  : "~1.0",
        "sensiolabs/security-checker"          : "~3.0",
        "scrutinizer/ocular"                   : "~1.1",
        "satooshi/php-coveralls"               : "~0.6",
        "chriskite/phactory"                   : "~0.4",
        "fzaninotto/faker"                     : "~1.5",
        "sami/sami"                            : "~3.0",
        "nikic/php-parser"                     : ">=1.0,<1.2"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\MantleBundle\\" : "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "2.1-dev"
        }
    }
}
